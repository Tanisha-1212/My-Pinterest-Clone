<% include  ./partials/header.ejs %>
<% include ./partials/navbar.ejs %>
    <div class="w-full mt-16 h-[90vh] bg-center bg-cover bg-[url('/images/pin-bg-1.jpg')]">
    <div class="flex items-center justify-center">
    <div class=" mt-10 w-[400px] bg-white px-4 py-5 rounded-4xl">        
        <div class="flex items-center justify-center mb-5">
            <img class="size-[40px] items-center" src="data:image/png;base64,"/>
        </div>
        <h1 class="text-4xl font-bold text-center mb-4 px-8">Welcome to Pinterest</h1>
        <form action="/login" method="POST">
            <div class="flex flex-col px-10 gap-2">
                <label for="email">Email</label>
                <input 
                type="email" 
                name="email" 
                placeholder="Email"
                class="rounded-xl p-2 border border-gray-400"/>

                <label for="password">Password</label>
                <input 
                type="password" 
                name="password" 
                placeholder="Password"
                class="rounded-xl p-2 border border-gray-400"/>
               
                <a class="text-blue-800 mb-2 ">Forgot your password?</a>
                <button 
                type="submit"
                class="bg-red-500 text-white p-2 rounded-xl">
                Continue
                </button>

                <p class="text-center text-[12px] text-gray-300">By continuing, you agree to Pinterest's Terms of Service and acknowledge you've read our Privacy Policy. Notice at collection.</p>

                <p style="font-family: Arial, Helvetica, sans-serif;" class="text-sm text-center">Not on Pinterest yet?<a href="/signup" class="decoration:none">Sign up</a></p>
            </div>
        </form>
    </div>
  </div>
  </div>

<% include  ./partials/footer.ejs %>