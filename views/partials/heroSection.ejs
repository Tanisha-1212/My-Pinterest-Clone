<body class="m-0 p-0 overflow-hidden">
  <div class="h-screen overflow-y-scroll snap-y snap-mandatory"
       style="-ms-overflow-style: none; scrollbar-width: none;">
<!--Top Section-->
<section class="relative h-screen bg-cover bg-center flex items-center justify-between p-10 snap-start" 
    style="background-image: url('/images/Screenshot 2025-08-31 200246.png');">
 
    <!-- Frosted Glass Background -->
  <div class="absolute inset-0 backdrop-blur-[2px] bg-white/30 z-0"></div>

  <!-- Left Side Text -->
  <div class="text-white max-w-md z-10">
    <h1 class="text-black text-4xl font-bold mb-4">Log in to get your ideas</h1>
    <p class="text-black text-lg">Discover, save, and share creative inspirations.</p>
  </div>

  <!-- Right Side Login Form -->
 <div class="flex items-center justify-center z-10">
    <div class=" mt-10 w-[400px] bg-white px-4 py-5 rounded-4xl">        
        <div class="flex items-center justify-center mb-5">
            <img class="size-[40px] items-center" src="data:image/png;base64,"/>
        </div>
        <h1 class="text-4xl font-bold text-center mb-4 px-8">Welcome to Pinterest</h1>
        <form action="">
            <div class="flex flex-col px-10 gap-2">
                <label for="email">Email</label>
                <input 
                type="email" 
                name="email" 
                placeholder="Email"
                class="rounded-xl p-2 border border-gray-400"/>

                <label for="password">Password</label>
                <input 
                type="password" 
                name="password" 
                placeholder="Password"
                class="rounded-xl p-2 border border-gray-400"/>
               
                <a class="text-blue-800 mb-2 ">Forgot your password?</a>
                <button 
                type="submit"
                class="bg-red-500 text-white p-2 rounded-xl">
                Continue
                </button>

                <p class="text-center text-[12px] text-gray-300">By continuing, you agree to Pinterest's Terms of Service and acknowledge you've read our Privacy Policy. Notice at collection.</p>

                <p style="font-family: Arial, Helvetica, sans-serif;" class="text-sm text-center">Not on Pinterest yet?<a href="/signup" class="decoration:none">Sign up</a></p>
            </div>
        </form>
    </div>
  </div>
</section>

<!--Section One-->
<section class="relative h-screen flex flex-col justify-center items-center text-center bg-yellow-200 snap-start">


  <!-- Text Content -->
  <div class="relative z-10">
    <h1 class="text-4xl md:text-6xl font-bold text-red-600">
      <span id="changingText" class="transition-all duration-700">Search for an Idea</span>
    </h1>
    <p class="mt-4 text-lg text-red-600 max-w-lg mx-auto">
      What do you want to try next? Think of something you are into!
    </p>
  </div>

  <!-- Stacked Images -->
  <div class="relative z-10 mt-12 flex flex-wrap justify-center gap-8">
    <img src="data:image/jpeg;base64," 
    class="w-40 h-40 rounded-xl shadow-lg transform rotate-[-8deg]" />
    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSmOvh2iaJPWcqgC7NUPjJ2JZSqUtXWgVDmPaAD1-6hmAocGTr_bkEXE0barVPyr9a01yA&usqp=CAU" 
    class="w-44 h-44 rounded-xl shadow-lg transform rotate-[4deg]" />
    <img src="data:image/jpeg;base64," 
    class="w-32 h-32 rounded-xl shadow-lg transform rotate-[12deg]" />

     <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQiOxTMIOmbgACt2GKQHOa6vPvmtrIT7MZiPw&s" 
     class="w-44 h-44 rounded-xl shadow-lg transform rotate-[6deg]" />
    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR0UcbYLFJAFeUe3qrw28uVQbQMFaOC4sk30w&s" 
    class="w-48 h-48 rounded-xl shadow-lg transform rotate-[-12deg]" />
  </div>
</section>


<!--Section Two-->

<section class="relative h-screen flex items-center justify-center bg-blue-200 px-10 snap-start">
  <div class="grid grid-cols-1 md:grid-cols-2 gap-10 items-center max-w-6xl">
    
    <!-- Text Section -->
    <div>
      <h1 class="text-5xl font-bold text-[#34a0a4]">Save ideas you like</h1>
      <p class="mt-4 text-lg text-[#34a0a4]">
        Explore inspirations with a gallery that feels alive.
      </p>
    </div>

    <!-- Image Cluster -->
    <div class="relative h-[400px] w-full">
      <img src="data:image/jpeg;base64," 
      class="absolute top-0 left-5 w-56 h-56 rounded-xl shadow-lg transform rotate-[-5deg]" />
      <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTg1vGoOVQ7mQhqkAG3hwpBOqP6IgL2Z3P9RQ&s" 
      class="absolute top-10 right-0 w-40 h-40 rounded-xl shadow-lg transform rotate-[8deg]" />
      <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQyjui9roSnfNjZJXOYR_KorO6YKqsRX83fMg&s" 
      class="absolute bottom-0 left-10 w-44 h-44 rounded-xl shadow-lg transform rotate-[12deg]" />
      <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQWI8YQCDKUGVbcyZ0a7pIb_CAFU8BMS0AerQ&s" 
      class="absolute bottom-10 right-12 w-40 h-40 rounded-xl shadow-lg transform rotate-[-10deg]" />
    </div>
  </div>
</section>

<!--Section Three-->
<section class="h-screen flex items-center justify-center bg-[#9c6644] px-10 snap-start">
  <div class="grid grid-cols-1 md:grid-cols-2 gap-10 items-center max-w-6xl">

    <!-- Text Section -->
    <div>
      <h2 class="text-5xl font-bold text-[#e6ccb2]">
        See it, make it,
      </h2>
      <h2 class="text-5xl font-bold text-[#e6ccb2]">
        make it, do it
      </h2>
      <p class="mt-4 text-lg text-[#e6ccb2]">
        Explore creativity through layered visuals and modern layouts.
      </p>
    </div>

    <!-- Image Section -->
    <div class="relative h-[400px] w-full">
      <!-- Base Image -->
      <img src="https://images.unsplash.com/photo-1611826585949-b0ccabd2c1a4?w=600&auto=format&fit=crop&q=60&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MjR8fG1ha2V1cHxlbnwwfHwwfHx8MA%3D%3D" 
      class="w-3/4 h-full object-cover rounded-2xl shadow-xl" />

      <!-- Overlapping Image -->
      <img src="https://images.unsplash.com/photo-1512496015851-a90fb38ba796?w=600&auto=format&fit=crop&q=60&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxzZWFyY2h8NHx8bWFrZXVwfGVufDB8fDB8fHww" 
      class="absolute top-50 left-2/3 w-1/2 h-1/2 object-cover rounded-2xl shadow-xl border-4 border-white" />
    </div>
  </div>
</section>
</div>
</body>



